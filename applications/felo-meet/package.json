    "main": "webpack.config.web.js",
    "scripts": {
        "build:web": "cross-env NODE_ENV=production webpack --config scripts/webpack.config.web.js",
        "profile:web": "cross-env NODE_ENV=production webpack --profile --json --config scripts/webpack.config.web.js > ./dist/profile.json && webpack-bundle-analyzer ./dist/profile.json",
        "start:web": "cross-env WEBPACK_IS_DEV_SERVER=true NODE_ENV=development webpack serve --config scripts/webpack.config.babel.js",
        "release:web": "npm version patch",
        "android": "react-native run-android",
        "build:ios": "cross-env PLATOS=ios NODE_ENV=production react-native run-ios",
        "build:android": "cross-env PLATOS=android NODE_ENV=production react-native run-android",
        "ios": "cross-env PLATOS=ios react-native run-ios",
        "start": "cross-env PLATOS=IOS react-native start",
        "test": "jest",
        "lint": "eslint ."
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "post-commit": "git update-index --again"
        }
